{
  "name": "react-viewport-slider",
  "version": "0.0.5",
  "description": "Viewport Slider React component.",
  "main": "./dist/modules/index.js",
  "scripts": {
    "bundle-demo": "webpack --config webpack.config.demo.js",
    "bundle-dist": "webpack --config webpack.config.dist.js",
    "bundle-modules": "trash dist/modules/* --force && babel src --loose --out-dir dist/modules",
    "bundle": "npm run bundle-demo && npm run bundle-dist && npm run bundle-modules",
    "demo": "webpack-dev-server --config webpack.config.demo.js",
    "lint": "eslint ./src",
    "test": "npm run lint && mocha -R spec --compilers jsx:test/compiler.js ./src/*Test.jsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daviferreira/react-viewport-slider.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "slider",
    "carousel",
    "fullscreen",
    "viewport",
    "responsive"
  ],
  "author": "Davi Ferreira <hi@daviferreira.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "0.14.0",
    "react-dom": "0.14.0-rc1"
  },
  "dependencies": {
    "classnames": "2.1.5",
    "react-scroll": "git://github.com/daviferreira/react-scroll"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-core": "5.8.25",
    "babel-loader": "5.3.2",
    "css-loader": "0.18.0",
    "eslint": "1.7.0",
    "jsdom": "6.5.1",
    "mocha-jsdom": "1.0.0",
    "normalize.css": "3.0.3",
    "react-addons-test-utils": "0.14.0-rc1",
    "sinon": "1.17.1",
    "style-loader": "0.12.3",
    "trash": "3.1.1",
    "unexpected": "10.0.1",
    "unexpected-sinon": "8.0.0",
    "webpack": "1.12.1",
    "webpack-dev-server": "1.10.1"
  }
}
